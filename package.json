{
  "name": "@jasonehmke/micro-stack",
  "version": "1.4.19",
  "description": "A library to interface with GCP Serverless Functions and PubSub",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "npm run lint && npm run cover",
    "test:prod": "cross-env BABEL_ENV=production npm run test",
    "test:only": "mocha --require babel-core/register --require babel-polyfill --recursive",
    "test:watch": "npm test -- --watch",
    "test:examples": "node examples/",
    "cover": "istanbul cover _mocha -- --require babel-core/register --require babel-polyfill --recursive",
    "build": "cross-env BABEL_ENV=production babel src --out-dir lib",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build",
    "bump": "bump --prompt --tag --push --all",
    "lint": "eslint src --fix",
    "release": "npm run upgrade && npm run bump && npm publish --access=public",
    "upgrade": "npm-check -u"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jayehmke/micro-stack.git"
  },
  "keywords": [],
  "author": "Jason Ehmke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flexdinesh/micro-stack/issues"
  },
  "homepage": "https://github.com/flexdinesh/micro-stack#readme",
  "dependencies": {
    "@google-cloud/datastore": "^3.0.1",
    "@google-cloud/pubsub": "^0.22.2",
    "babel-polyfill": "^6.26.0",
    "debug": "^4.1.1",
    "express-validator": "^5.3.1",
    "gstore-node": "^4.3.3",
    "joi": "^14.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.5.0",
    "babel-preset-node6": "^11.0.0",
    "chai": "^4.1.2",
    "codacy-coverage": "^2.0.3",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "del": "^3.0.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-modular": "^6.0.0",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.12.4",
    "istanbul": "^1.1.0-alpha.1",
    "mkdirp": "^0.5.1",
    "mocha": "^5.0.0",
    "npm-check": "^5.5.2",
    "rimraf": "^2.6.2",
    "through2": "^2.0.3",
    "version-bump-prompt": "^4.0.0"
  }
}